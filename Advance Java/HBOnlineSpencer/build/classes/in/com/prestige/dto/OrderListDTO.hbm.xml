<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="in.com.prestige.dto">
<class name="OrderListDTO" table="orderlist">
 <id name="orderid" column="ORDER_ID"> <generator class="increment"/></id>
 <property name="customersID" column="CUSTOMERSID"/>
 <property name="contactNo" column="CONTACT_NO"/>
 <property name="productName" column="PRODUCT_NAME"/>
 <property name="price" column="PRICE"/>
 <property name="quantity" column="QUANTITY"/>
</class>
</hibernate-mapping>